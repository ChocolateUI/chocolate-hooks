{
  "name": "chocolate-hooks",
  "version": "0.1.1",
  "description": "a hook library for work",
  "repository": "https://github.com/ChocolateUI/chocolate-hooks",
  "bugs": {
    "url": "https://github.com/ChocolateUI/chocolate-hooks/issues"
  },
  "homepage": "https://chocolate-hooks.netlify.com/",
  "author": "XueShiming <shiming_xsm@163.com>",
  "license": "MIT",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "keywords": [
    "react",
    "react-hooks"
  ],
  "sideEffects": [
    "dist/*"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/chocolate-hooks.min.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "yarn dev",
    "dev": "dumi dev",
    "build": "yarn clean && yarn build:cjs && yarn build:es && yarn build:umd",
    "lint": "eslint src --ext '.js,.jsx,.ts,.tsx'",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --project tsconfig.build.json --noEmit",
    "build:cjs": "tsc --project tsconfig.build.json -m commonjs --outDir lib --declaration",
    "build:es": "tsc --project tsconfig.build.json -m esNext --outDir es --declaration",
    "build:umd": "webpack --config webpack.umd.config.js",
    "clean": "rimraf lib es dist",
    "release": "semantic-release",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "ci:coverage": "npm run coverage && codecov",
    "clean:docs": "rimraf _docs",
    "build:docs": "yarn clean:docs && dumi build",
    "bundlesize": "bundlesize"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/chocolate-hooks.min.js",
      "maxSize": "100 kB"
    }
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint:types && yarn test"
    }
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "dependencies": {
    "@umijs/plugin-sass": "^1.1.1",
    "chocolate-ui": "^0.9.8",
    "history": "^5.0.0",
    "react-router-dom": "^5.2.0"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.10.4",
    "@babel/plugin-proposal-optional-chaining": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@commitlint/config-conventional": "9.1.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@testing-library/react-hooks": "3.4.1",
    "@types/enzyme": "3.10.5",
    "@types/jest": "26.0.10",
    "@types/js-cookie": "^2.2.7",
    "@types/lodash": "4.14.170",
    "@types/react-router-dom": "5.1.5",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "antd": "4.6.1",
    "awesome-typescript-loader": "5.2.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "1.13.3",
    "bundlesize": "0.18.0",
    "codecov": "3.7.2",
    "commitlint": "9.1.2",
    "css-loader": "^5.2.0",
    "dumi": "1.1.10",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.7.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "9.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "husky": "4.2.5",
    "jest": "26.4.2",
    "js-cookie": "^3.0.0",
    "lint-staged": "10.2.13",
    "lodash": "4.17.20",
    "prettier": "2.1.1",
    "progress-bar-webpack-plugin": "2.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "sass-loader": "^11.0.1",
    "semantic-release": "17.1.1",
    "standard-version": "8.0.0",
    "style-loader": "^2.0.0",
    "typescript": "3.9.7",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "4.44.1",
    "webpack-cli": "4.7.0"
  }
}
